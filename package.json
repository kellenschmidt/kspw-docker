{
  "name": "kspw-docker",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "config": {
    "docker_file": "./config/dev/docker-compose.yml",
    "docker_file_prod": "./config/prod/docker-compose.yml"
  },
  "scripts": {
    "install": "sh bin/install.sh docker",
    "docker-start": "bash ./bin/start.sh $npm_package_config_docker_file",
    "docker-deploy-test": "sh bin/install.sh test && bash ./bin/start.sh $npm_package_config_docker_file",
    "docker-deploy-prod": "sh bin/install.sh prod && bash ./bin/start.sh $npm_package_config_docker_file",
    "docker-stop": "bash ./bin/stop.sh $npm_package_config_docker_file"
  }
}
